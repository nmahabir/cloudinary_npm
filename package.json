  "scripts": {
    "test": "node_v=$(node --version)ยง\nif [[ ${node_v%%.*ยง} == 'v4' ]]\nthen\necho 4 && npm run test4\nelse\necho 10 && npm run test10\nfi",
    "compile": "node_modules/.bin/babel lib --out-dir lib-node4 --delete-dir-on-start --verbose",
    "test10": "which node && node node_modules/.bin/mocha --ui bdd -R spec --recursive test/",
    "test4": "node node_modules/.bin/mocha -R spec --recursive --require 'babel-register' --require 'babel-polyfill' test/",
